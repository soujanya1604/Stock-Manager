@model PortfolioStock

<p>Are you sure you want to delete this entry from your stock list?</p>

<div>
    <strong>Date:</strong> @Model.Date.ToString("yyyy-MM-dd")
</div>
<form method="post">
    <button type="submit" class="btn btn-danger">Delete Stock</button>
    <a href="@Url.Action("GetStockPrice", "Stock", new { symbol = Model.Stock.Symbol })" class="btn btn-secondary">Cancel</a>
</form>
